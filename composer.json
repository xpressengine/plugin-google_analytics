{
  "name": "xpressengine-plugin/google_analytics",
  "description": "XpressEngine's plugin for bundle (XE)",
  "keywords": ["xpressengine bundle", "analytics"],
  "version": "0.9.2",
  "support": {
    "email": "developers@xpressengine.com"
  },
  "authors": [
    {
      "name": "XE Developers",
      "email": "developers@xpressengine.com",
      "homepage": "http://xpressengine.io",
      "role": "Developer"
    }
  ],
  "license": "LGPL-2.1",
  "type": "xpressengine-plugin",
  "extra": {
    "xpressengine": {
      "title": "Google Analytics",
      "icon": "icon.png",
      "component": {
        "google_analytics/skin/google_analytics@default": {
          "class": "Xpressengine\\Plugins\\GoogleAnalytics\\Skins\\ManagerSkin",
          "name": "설정스킨",
          "description": "관리자 설정페이지 스킨입니다."
        },
        "widget/google_analytics@analytics": {
          "class": "Xpressengine\\Plugins\\GoogleAnalytics\\AnalyticsWidget",
          "name": "Analytics 위젯",
          "description": "Analytics api 위젯 입니다."
        },
        "widget/google_analytics@analytics/skin/google_analytics@default": {
          "class": "Xpressengine\\Plugins\\GoogleAnalytics\\Skins\\AnalyticsWidgetSkin",
          "name": "Analytics 위젯 기본 스킨",
          "description": "Analytics 위젯 기본 스킨입니다."
        }
      }
    }
  },
  "require": {
    "google/apiclient": "1.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Xpressengine\\Plugins\\GoogleAnalytics\\": "src/"
    }
  }
}
